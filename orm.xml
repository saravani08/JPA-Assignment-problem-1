<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm    
   http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
   version="1.0">
   <entity class="com.Ticket">
       <table name="Ticket">
       
       </table>
       <attributes>
          <id name="source">
          </id>
          <basic name="price">
          <column name="Ticketprice"/>
          </basic>
       </attributes>
   </entity>
    <entity class="com.Person">
       <table name="Person">
       
       </table>
       <attributes>
          <id name="personName">
          </id>
          <basic name="Name">
          <column name="personName"/>
          </basic>
       </attributes>
   </entity>
   <entity class="com.Employee">
       <table name="Employee_details">
       
       </table>
       <attributes>
          <id name="employeeName">
          </id>
          <basic name="Name">
          <column name="employeeName"/>
          </basic>
       </attributes>
   </entity>
   <entity class="com.Student">
       <table name="Student">
       
       </table>
       <attributes>
          <id name="studentName">
          </id>
          <basic name="studentName">
          <column name="studentName"/>
          </basic>
       </attributes>
   </entity>    
</entity-mappings>
